{
  "name": "nest-sql",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=dev node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "NODE_ENV=dev npm run typeorm -- migration:generate -d src/config/typeorm-config.ts",
    "migration:run": "NODE_ENV=dev npm run typeorm -- migration:run -d src/config/typeorm-config.ts",
    "migration:revert": "NODE_ENV=dev npm run typeorm -- migration:revert -d src/config/typeorm-config.ts",
    "migration:create": "NODE_ENV=dev npm run typeorm migration:create",
    "migration:show": "NODE_ENV=dev npm run typeorm migration:show -d src/config/typeorm-config.ts",
    "migration:generate:prod": "NODE_ENV=prod npm run typeorm -- migration:generate -d src/config/typeorm-config.ts",
    "migration:run:prod": "NODE_ENV=prod npm run typeorm -- migration:run -d src/config/typeorm-config.ts",
    "migration:revert:prod": "NODE_ENV=prod npm run typeorm -- migration:revert -d src/config/typeorm-config.ts",
    "migration:create:prod": "NODE_ENV=prod npm run typeorm migration:create",
    "migration:show:prod": "NODE_ENV=prod npm run typeorm migration:show -d src/config/typeorm-config.ts",
    "build:scripts": "tsc -p tsconfig.json src/scripts/migration-renamer.ts --outDir dist/scripts",
    "migration:rename": "NODE_ENV=dev ts-node src/scripts/migration-renamer.ts",
    "migration:rename:prod": "NODE_ENV=prod ts-node src/scripts/migration-renamer.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.47",
    "mysql2": "^3.12.0",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
